   "source": [
    "n=50\n",
    "a=0\n",
    "i=0\n",
    "while n!=0:\n",
    "    bit = n&1   # n&1 will give us the last bit\n",
    "    a=bit*(10**i)+a # it is the formula \n",
    "    n=n>>1 # it will right shift every  digit\n",
    "    i+=1\n",
    "print(a)\n",
    "    \n",
    "        "
